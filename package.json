{
  "name": "@redsift/d3-rs-tip",
  "version": "0.10.0",
  "description": "Tip for graphs using D3v4.",
  "keywords": [
    "redsift",
    "d3",
    "tip"
  ],
  "homepage": "https://github.com/redsift/d3-rs-tip",
  "license": "MIT",
  "author": {
    "name": "Christos Vontas"
  },
  "module": "dist/d3-rs-tip.esm.js",
  "jsnext:main": "dist/d3-rs-tip.esm.js",
  "main": "dist/d3-rs-tip.umd.min.js",
  "browser": "dist/d3-rs-tip.umd.min.js",
  "esnext": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redsift/d3-rs-tip.git"
  },
  "scripts": {
    "serve": "gulp -o d3-rs-tip serve",
    "prepublish": "npm run clean; npm run build",
    "build": "npm run clean; node ./node_modules/@redsift/rollup-bundler/src/index.js bundle.config.js",
    "clean": "rm -rf dist",
    "pretest": "(npm outdated || true) && npm run build",
    "test": "tape 'test/**/*-test.js' && eslint src"
  },
  "dependencies": {
    "@redsift/d3-rs-theme": "^0.6.0",
    "d3-selection": "^1.4.2",
    "d3-transition": "^1.3.2"
  },
  "devDependencies": {
    "@redsift/rollup-bundler": "^2.0.2",
    "@redsift/tape-reel": "~0.1.1",
    "browser-sync": "^2.29.3",
    "del": "^2.2.2",
    "eslint": "^8.47.0",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^5.0.0"
  },
  "overrides": {
    "lodash": "4.17.21",
    "lodash.template": "4.5.0",
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2",
    "minimist": "1.2.6"
  }
}
